@model IDataResult<List<Posts>>
<link href="~/Views/Posts/css/post.css" rel="stylesheet" />
@{
    List<string> Months = new List<string>
    {
        "Ocak",
        "Şubat",
        "Mart",
        "Nisan",
        "Mayıs",
        "Haziran",
        "Temmuz",
        "Ağustos",
        "Eylül",
        "Ekim",
        "Kasım",
        "Aralık"
    };
}

<div class="event-schedule-area-two bg-color pad100">
    <div class="container">

        <div class="row">

            <div class="col-lg-12">

                <ul class="nav custom-tab mt-5" id="myTab" role="tablist">


                    @foreach (var item in Model.Data.Select(x => x.Post_CreatedDate))
                    {
                        <li class="nav-item">

                            <a class="nav-link active show days" data-toggle="tab" href="#home" role="tab" data="item" aria-controls="home" aria-selected="true">
                                <span>@item.Day @Months[item.Month - 1]</span>
                            </a>

                        </li>
                    }

                </ul>

                <div class="tab-content" id="myTabContent">

                    <div class="tab-pane fade active show" id="home" role="tabpanel">

                        <div class="table-responsive">

                            <table class="table">

                                <thead>

                                    <tr>
                                        <th class="text-center" scope="col">Tarih</th>
                                        <th scope="col">Şehir</th>
                                        <th scope="col">Plaka</th>
                                        <th scope="col">Puan</th>
                                        <th scope="col">Açıklama</th>
                                        <th scope="col"></th>
                                    </tr>

                                </thead>

                                <tbody>

                                    @foreach (var item in Model.Data)
                                    {
                                        <tr class="inner-box">
                                            <th scope="row">
                                                <div class="event-date">
                                                    <span>@item.Post_CreatedDate.Date.Day</span>
                                                    <p>@Months[item.Post_CreatedDate.Month-1]</p>
                                                </div>
                                            </th>
                                            <td>
                                                <div class="event-wrap">
                                                    <h3><a href="#">@item.City.City_Name</a></h3>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="event-wrap">
                                                    <h3><a href="#">@item.Post_Plate</a></h3>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="r-no">
                                                    <span>Room B3</span>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="r-no">
                                                    <span>@item.Post_Description</span>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="primary-btn">
                                                    <a class="btn btn-primary" href="#">Read More</a>
                                                </div>
                                            </td>
                                        </tr>
                                    }

                                </tbody>
                            </table>
